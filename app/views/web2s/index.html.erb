<h1>Listing web2s</h1>

<table>
  <tr>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @web2s.each do |web2| %>
  <tr>
    <td><%= web2.url %></td>
    <td><%= link_to 'Show', web2 %></td>
    <td><%= link_to 'Edit', edit_web2_path(web2) %></td>
    <td><%= link_to 'Destroy', web2, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Web2', new_web2_path %>
